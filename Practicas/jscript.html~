<script type="text/javascript">
for (i=1;i<=10;i++)
{
	
	document.write("<table border ='1'><caption>Tabla del " + i + "</caption>");
	for(j=1; j<=10; j++)
	{
		document.write("<tr>" +
							"<td>" + i + "</td>" + 
							"<td>" + j + "</td>" +
							"<td>" + i*j + "</td>" +
							"</tr>");	
							
	}
	document.write("</table>");
}
</script>
